{
  "version": "0.1.0",
  "name": "sentinel_vault",
  "instructions": [
    {
      "name": "initialize",
      "accounts": [
        { "name": "authority", "isMut": true, "isSigner": true },
        { "name": "sentinelConfig", "isMut": true, "isSigner": false },
        { "name": "systemProgram", "isMut": false, "isSigner": false }
      ],
      "args": [
        { "name": "oldTokenMint", "type": "publicKey" },
	      { "name": "dexProgramId", "type": "publicKey" }
      ]
    },
    {
      "name": "deposit",
      "accounts": [
        { "name": "user", "isMut": true, "isSigner": true },
        { "name": "userVault", "isMut": true, "isSigner": false },
        { "name": "sentinelConfig", "isMut": true, "isSigner": false },
        { "name": "userOldTokenAccount", "isMut": true, "isSigner": false },
        { "name": "vaultOldTokenAccount", "isMut": true, "isSigner": false },
        { "name": "tokenProgram", "isMut": false, "isSigner": false },
        { "name": "systemProgram", "isMut": false, "isSigner": false }
      ],
      "args": [
        { "name": "amount", "type": "u64" }
      ]
    },
    {
      "name": "withdraw",
      "accounts": [
        { "name": "user", "isMut": true, "isSigner": true },
        { "name": "userVault", "isMut": true, "isSigner": false },
        { "name": "sentinelConfig", "isMut": true, "isSigner": false },
        { "name": "vaultAuthority", "isMut": false, "isSigner": false },
        { "name": "vaultOldTokenAccount", "isMut": true, "isSigner": false },
        { "name": "userOldTokenAccount", "isMut": true, "isSigner": false },
        { "name": "tokenProgram", "isMut": false, "isSigner": false }
      ],
      "args": [
        { "name": "amount", "type": "u64" }
      ]
    }
  ],
  "accounts": [
    {
      "name": "SentinelConfig",
      "type": {
        "kind": "struct",
        "fields": [
          { "name": "authority", "type": "publicKey" },
          { "name": "oldTokenMint", "type": "publicKey" }
        ]
      }
    },
    {
      "name": "UserVault",
      "type": {
        "kind": "struct",
        "fields": [
          { "name": "user", "type": "publicKey" },
          { "name": "oldTokenDeposited", "type": "u64" }
        ]
      }
    }
  ],
  "types": [],
  "events": [],
  "errors": [
    { "code": 6000, "name": "Unauthorized", "msg": "You are not authorized to perform this action." },
    { "code": 6001, "name": "AlreadyInitialized", "msg": "The vault is already initialized." },
    { "code": 6002, "name": "InsufficientFunds", "msg": "Insufficient funds." }
  ]
}
